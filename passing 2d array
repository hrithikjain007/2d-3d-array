// passing 2d array

#include<stdio.h>
void pass(int (*p)[3])
{
	int i,j;
	for(i=0;i<2;i++)
	{
		for(j=0;j<3;j++)
		{
			printf("%d",p[i][j]);
		}
		printf("\n");
	}
}
void main()

	{
		int a[2][3],i,j;
		printf("\n enter the elements\n");
		for(i=0;i<2;i++)
		{
			for(j=0;j<3;j++)
			{
				scanf("%d",&a[i][j]);
			}
		}



		pass(a);
	}


     
